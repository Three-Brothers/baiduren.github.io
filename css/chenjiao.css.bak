} */
/* 四川农业大学专属样式 */
#SCNY table span {
	color: #f10215;
	font-weight: 600;
	font-size: 17px;
}
#SCNY table {
	margin-bottom: 30px ;
}
#SCNY #FJ {
	line-height: 40px;
	margin: 28px auto;
	text-align: -webkit-left;
}
#SCNY .content div {
	text-align: left;
	position: relative;
	left: 48px;
}
#SCNY .content a {
	z-index: 3;
}
#SCNY h5 {
	line-height: 40px;
	margin-top: 168px;
	border-top: 1px dashed #000;
}
/* 四川电大专属样式begin */
#SCDD div.zhuan {
	background-color: rgb(142,182,59);
	border: 1px solid #0d0101;
	width: 1000px;
	/* height: 44px; */
	margin: 6px auto;
	box-sizing: border-box;
}
.zhuan p {
	font-size: 26px;
	margin: 8px auto;
	text-align: center;
	color: #fff;
	line-height: 44px;
}
#SCDD table td:first-child {
	width: 96px;
}
#SCDD table td:last-child {
	text-align: left;
}
#SCDD .table {
	margin-bottom: 3px;
}
/* 川北医学院begin */

#CBYXY .code {
	text-align: center;
	font-size: 18px;
	font-weight: 600;
	margin: 10px auto;
}
#CBYXY table th:first-child {
	width: 48px;
}
.tab #CBYXY {
	margin: -3px auto;
}



/* 继续教育专属底部导航栏样式 */
footer.footer .summary .copy {
	margin-top: -6px;
	top: -4px;
}