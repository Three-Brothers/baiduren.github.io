/* 各类型展示区域版心 */
.edu {
	width: 1000px;
	margin: 0 auto;
	padding-bottom: 40px;
}
.edu #LOGO {
	height: 150px;
	width: 150px;
	position: relative;
	top: 16px;
	left: 58px;
}
#LOGO h2 {
	width: 100%;
	height: 100%;
	overflow: hidden;
}
#LOGO p {
	text-indent: -999px;
}
#LOGO a {
	display: block;
	heoght: 100%;
	position: absolute;
	top: 0;
	left: 0;
}
#LOGO img {
	width: 150px;
}

.edu h3 {
	font-size: 22px;
	line-height: 0;
	position: relative;
	top: -20px;
	left: 46px;
}

/* 各学校表格表头样式 */
.text th,
.text td{
	font-size: 16px;
	line-height: 20px;
	padding: 6px 20px;
}
/* 表格内文本对齐方式样式 */
table#DQ {	/* 设置id-固定该id为左对齐特性 */
	text-align: left;	/* 文本对齐方式-居左 */
}

table th,
table td {
    font-size: 14px;
    line-height: 20px;
    padding: 8px 6px;
}

/* 各单页H4统一格式begin */
