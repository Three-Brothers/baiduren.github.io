/* 业务详细专属样式 */
p.dw {
	top: -30px;
}
div.top {
	border-bottom: 2px solid #2196F3;
	/* box-shadow: 0 2px 10px #ccc; */
}
body div.nav {
	width: 68px;
	height: 30px;
	line-height: 30px;
	border: 1px solid red;
	background-color: #fff;
	position: relative;
	top: -44px;
	left: 44px;
	z-index: 2;
}
div.nav a {
	width: 100%;
	height: 100%;
	display: inline-block;
	text-indent: 10px;
	color: #f10215;
	z-index: 3;
}
div.nav a:hover {
	color: #fff;
	font-weight: 600;
}
div.nav:hover {
	background-color: #f10215;
}

